- title "Wishlists"
%table
  %tr
    %th Name
    %th User
  - for wishlist in @wishlists
    %tr
      %td= wishlist.name
      %td= wishlist.user_id
      %td= link_to "Show", wishlist
      %td= link_to "Edit", edit_wishlist_path(wishlist)
      %td= link_to "Destroy", wishlist, :confirm => 'Are you sure?', :method => :delete
%p= link_to "New Wishlist", new_wishlist_path
